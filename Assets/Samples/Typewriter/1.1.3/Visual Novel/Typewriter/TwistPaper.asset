        OnApply: []
        OnInvoke: []
        Speed: 1
        IsChoice: 0
        IsPlayerLocked: 0
        Text: Got it! 1 for blob, 2 for paper, 3 for scissors and 4 for rock. Easy
          enough to remember.
        _speaker:
          InternalID: 303196
    - rid: 669309055316263242
      type: {class: DialogueEntry, ns: , asm: Assembly-CSharp}
